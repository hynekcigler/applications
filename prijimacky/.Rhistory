## korelace
r_osp_zsv <- .71 ## korelace OSP a ZSV v celém vzorku včetně nepřijatých
r_osp_prospech <- .5 ## korelace OSP a prospěchu v celém vzorku včetně nepřijatých
r_zsv_prospech <- .5 ## korelace ZSV a prospěchu v celém vzorku včetně nepřijatých
## váha ZSV
vaha_zsv <- .6 ## váha ZSV
cormat <- matrix(c(1, r_osp_zsv, r_osp_prospech,
r_osp_zsv, 1, r_zsv_prospech,
r_osp_prospech, r_zsv_prospech, 1), ncol=3)
dimnames(cormat) <- list(c("osp", "zsv", "prospech"), c("osp", "zsv", "prospech"))
dat <- as.data.frame(sim.correlation(cormat, n = 12029, data = TRUE))
dat$sum <- vaha_zsv*dat$zsv + (1-vaha_zsv)*dat$osp
cutsc <- quantile(dat$sum, 1-441/12029)
dat$prijat <- (dat$sum >= cutsc)
prijati <- dat[dat$prijat == 1, ]
cor(prijati)
